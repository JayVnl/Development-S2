{
  "manifest_version": 2,
  "name": "EZ-Dice",
  "version": "1.0",
  "description": "Just a simple tool to roll a dice quickly in chrome",
  "icons": {
    "128": "dice128.png",
    "48": "dice48.png",
    "16": "dice16.png"
  },
  "browser_action": {
    "default_icon": "d6.png",
    "default_popup": "popup.html",
    "default_title": "EZ-Dice"
  }
}
